def bytos(so):
	source=so
	
	do={}
	for i in source.split('bytes'):
		
		
		try:
			sb=str(i).split('bytes(')[0]
			sb=sb.split(').decode()')[0]
			enc='bytes('+sb+').decode()'
			ev=(sb.replace('''
    ''',''''''))
			ls=[]
			for e in ev.split('\n'):
				e=e.replace(',','')
				e=e.replace(']','')
				e=e.replace('[','')
				try:
					en=(int(e))
					ls.append(en)
				
				
				except:
					continue
		
		
			rel=(bytes(ls).decode())
			enc=enc
			do.update({enc:rel})
			ls.clear
		except IndexError:
			continue
		#print(rel)

	for en,de in do.items():
		#source=open(fi,'r').read()
		if en in source:
			source=source
			#print(de)
			source=source.replace(en,f"'''{de}'''")
		source=source
		#source=open(fi,'w').write(source)
		save(source,'w','ou')
		
